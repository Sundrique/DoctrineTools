{
    "name": "sundrique/doctrine-tools",
    "description": "Zend Framework 2 Module unified endpoint for Doctrine command line tools",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
		"tools",
		"cli",
        "module",
        "zf2"
    ],
    "homepage": "https://github.com/Sundrique/DoctrineTools",
    "authors": [
        {
            "name": "Aleksandr Sandrovskiy",
            "email": "a.sandrovsky@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
		"doctrine/common": ">=2.0",
		"doctrine/orm": ">=2.0",
		"doctrine/dbal": ">=2.0",
		"doctrine/doctrine-module": ">=0.7",
		"doctrine/doctrine-orm-module": ">=0.7",
		"doctrine/migrations": "dev-master",
		"symfony/console": ">=2.0.13",
		"zendframework/zend-stdlib": "2.*",
		"zendframework/zend-mvc": "2.*",
		"zendframework/zend-servicemanager": "2.*"
    },
	"minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "DoctrineTools\\": "src/",
            "DoctrineToolsTest\\": "tests/"
        }
    }
}
